<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" 
  		android:state_selected="true" 
        android:state_checkable="true"
        android:state_checked="true" >
    <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
    <solid android:color="@color/colorRowGreen" />
	<stroke android:color="@color/borderExpired" android:width="1px" />
	<corners 
		android:bottomRightRadius="@dimen/p8dp"
        android:bottomLeftRadius="@dimen/p8dp"
        android:topRightRadius="@dimen/p8dp"
        android:topLeftRadius="@dimen/p8dp"/>
	</shape>
  </item>
  <item>
    <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
    <solid android:color="@color/colorSort" />
	<stroke android:color="@color/borderExpired" android:width="1px" />
	<corners 
		android:bottomRightRadius="@dimen/p8dp"
        android:bottomLeftRadius="@dimen/p8dp"
        android:topRightRadius="@dimen/p8dp"
        android:topLeftRadius="@dimen/p8dp"/>
	</shape>
  </item>
</selector>